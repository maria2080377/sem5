#include <iostream>    //Задача 2
#include <vector> 
#include <algorithm>
#include <numeric>
using namespace std;
int main() {
	setlocale(LC_ALL, "RU");
	vector <int> vec(10);

	for_each(vec.begin(), vec.end(), [](int& x) {cin >> x;});
	cout << endl;
	for_each(vec.begin(), vec.end(), [](int x) {cout << x*2 << " "; });

	return 0;
}
